<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Gym\Infrastructure\ExerciseToTraining\ExerciseToTraining" table="exercises_to_trainings">
        <id name="id" type="string" column="id"/>
        <field name="trainingId" type="string" column="training_id"/>
        <field name="stationId" type="string" column="station_id"/>
        <field name="exerciseId" type="string" column="exercise_id"/>
        <field name="status" type="string" column="status"/>
        <field name="seriesGoal" type="integer" column="series_goal"/>
        <field name="repetitionGoal" type="integer" column="repetition_goal"/>
        <field name="kilogramGoal" type="integer" column="kilogram_goal"/>
        <field name="createdAt" type="datetime" column="created_at"/>
    </entity>

</doctrine-mapping>